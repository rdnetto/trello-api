From: Reuben D'Netto <rdnetto@gmail.com>
Date: Tue, 5 Feb 2019 19:18:00 +1100
Subject: [PATCH] Fixed inconsistencies in path params

---
 swagger.yaml | 42 +++++++++++++++++++++++++++++++++---------
 1 file changed, 33 insertions(+), 9 deletions(-)

diff --git a/swagger.yaml b/swagger.yaml
index 29508ad..35d534a 100644
--- a/swagger.yaml
+++ b/swagger.yaml
@@ -741,7 +741,7 @@ paths:
       - required: true
         schema:
           type: string
-        in: query
+        in: path
         name: idMembership
         description: The id of a membership that should be added to this board.
       - required: true
@@ -1155,7 +1155,7 @@ paths:
       deprecated: false
   /boards/{boardId}/boardStars:
     get:
-      summary: /boards/{id}/boardStars
+      summary: /boards/{boardId}/boardStars
       _method: get
       parameters:
       - required: true
@@ -1168,7 +1168,7 @@ paths:
         schema:
           default: mine
           type: string
-        in: path
+        in: query
         name: filter
         description: ! 'Valid values: mine, none'
       _path: /boards/{boardId}/boardStars
@@ -1690,7 +1690,7 @@ paths:
         schema:
           default: none
           type: string
-        in: query
+        in: path
         name: idMember
         description: An ID of a member resource.
       - required: false
@@ -2138,6 +2138,12 @@ paths:
         in: path
         name: id
         description: The ID or username of the member
+      - required: true
+        schema:
+          type: string
+        in: path
+        name: idSearch
+        description: The ID of the search
       _path: /members/{id}/savedSearches/{idSearch}
       operationId: membersidsavedsearchesidsearch
       description: Get a saved search
@@ -2176,6 +2182,12 @@ paths:
         in: path
         name: id
         description: The ID or username of the member
+      - required: true
+        schema:
+          type: string
+        in: path
+        name: idSearch
+        description: The ID of the search
       - required: false
         schema:
           type: string
@@ -2962,6 +2974,12 @@ paths:
         in: path
         name: id
         description: The ID or username of the member
+      - required: true
+        schema:
+          type: string
+        in: path
+        name: idBackground
+        description: The ID of the background
       - required: false
         schema:
           type: string
@@ -3686,6 +3704,12 @@ paths:
           type: string
         in: path
         name: id
+        description: The ID of the background
+      - required: true
+        schema:
+          type: string
+        in: path
+        name: idBackground
         description: The ID or username of the member
       - required: false
         schema:
@@ -5866,7 +5890,7 @@ paths:
       deprecated: false
   /enterprises/{id}/organizations/{idOrganization}:
     delete:
-      summary: /enterprises/{id}/admins/{idMember}
+      summary: /enterprises/{id}/organizations/{idOrganization}
       _method: delete
       parameters:
       - required: true
@@ -5879,8 +5903,8 @@ paths:
         schema:
           type: string
         in: path
-        name: idMember
-        description: ID of the member to be removed as an admin from enterprise.
+        name: idOrganization
+        description: ID of the organization
       _path: /enterprises/{id}/organizations/{idOrganization}
       operationId: enterprisesidadminsidmember-2
       description: Remove an member as admin from an enterprise.
@@ -7295,14 +7319,14 @@ paths:
         schema:
           default: all
           type: string
-        in: path
+        in: query
         name: fields
         description: ! '`all` or a comma-separated list of attachment [fields](ref:attachments)'
       - required: true
         schema:
           default: 'false'
           type: string
-        in: path
+        in: query
         name: filter
         description: Use `cover` to restrict to just the cover attachment
       _path: /cards/{id}/attachments
